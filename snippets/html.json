{
	/* Core Elements */
	"Vaadin Combo Box": {
		"prefix": "vaadin-combo-box",
		"body": [
			"<vaadin-combo-box label=\"$1\" items=\"$2\"></vaadin-combo-box>"
		],
		"description": "Combo Box Element"
	},
	"Vaadin Date Picker": {
		"prefix": "vaadin-date-picker",
		"body": [
			"<vaadin-date-picker label=\"$1\"></vaadin-date-picker>"
		],
		"description": "Date Picker Element"
	},
	"Vaadin Context Menu": {
		"prefix": "vaadin-context-menu",
		"body": [
			"<vaadin-context-menu>",
			"\t<template>",
			"\t\t<paper-menu>",
			"\t\t\t<paper-item>Item 1</paper-item>",
			"\t\t\t<paper-item>Item 2</paper-item>",
			"\t\t<paper-menu>",
			"\t</template>",
			"\t<a href=\"#\" id=\"opener\">Right Click to open</a>",
			"</vaadin-context-menu>"
		],
		"description": "Context Menu Element"
	},
	"Vaadin Grid": {
		"prefix": "vaadin-grid",
		"body": [
			"<vaadin-grid>",
			"\t<table>",
			"\t\t<colgroup>",
			"\t\t\t<col name=\"$1\">",
			"\t\t\t<col name=\"$2\">",
			"\t\t\t<col name=\"$3\">",
			"\t\t</colgroup>",
			"\t</table>",
			"</vaadin-grid>"
		],
		"description": "Datagrid Element"
	},
	"Vaadin Split Layout": {
		"prefix": "vaadin-split-layout",
		"body": [
			"<vaadin-split-layout>",
			"\t<div>$1</div>",
			"\t<div>$2</div>",
			"</vaadin-split-layout>"
		],
		"description": "Split Layout Element"
	},
	"Vaadin Upload": {
		"prefix": "vaadin-upload",
		"body": [
			"<vaadin-upload target=\"$1\"></vaadin-upload>"
		],
		"description": "File Upload Element"
	},

	/* Charts */

	"Vaadin Area Chart": {
		"prefix": "vaadin-area-chart",
		"body": [
			"<vaadin-area-chart>",
			"\t<chart-title>${1:Fibonacci}</chart-title>",
			"\t<x-axis>",
			"\t\t<title>${2:Index}</title>",
			"\t</x-axis>",
			"\t<y-axis>",
			"\t\t<title>${3:Value}</title>",
			"\t</y-axis>",
			"\t<data-series>",
			"\t\t<data>${4:1, 1, 2, 3, 5, 8, 13, 21, 34}</data>",
			"\t</data-series>",
			"\t<data-series>",
			"\t\t<data>${5:34, 21, 13, 8, 5, 3, 2, 1, 1}</data>",
			"\t</data-series>",
			"</vaadin-area-chart>"
		],
		"description": "Area Chart Element"
	},
